#!groovy

properties([disableConcurrentBuilds()])

node('backend'){
    if ($ref == 'refs/heads/backend') {
              sh 'echo back'
    } else {
              sh 'echo notback'
    }
}

node('frontend'){
    if ($ref == 'refs/heads/frontend') {
              sh 'echo front'
    } else {
              sh 'echo notfront'
    }
}
