#!groovy

properties([disableConcurrentBuilds()])

node('Backend'){
    if ( $ref == 'refs/heads/backend' ) {
              sh 'echo $ref'
    } else {
              sh 'echo notback'
    }
}

node('Frontend'){
    if ( $ref == 'refs/heads/frontend') {
              sh 'echo $ref'
    } else {
              sh 'echo notfront'
    }
}
