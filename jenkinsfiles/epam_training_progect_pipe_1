#!groovy

properties([disableConcurrentBuilds()])

pipeline {
    agent { 
        label 'backend'
        }
    options {
        buildDiscarder(logRotator(numToKeepStr: '10', artifactNumToKeepStr: '5'))
        timestamps()
    }
    stages {
        stage("First step") {
            steps {
                sh 'ip a'
                sh 'echo $ref'
            }
        }
       
    }
}
