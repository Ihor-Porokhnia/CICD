#!groovy

properties([disableConcurrentBuilds()])

node('Backend'){
    sh 'echo $ref'
   // if ( $ref == 'refs/heads/backend' )
    if (true)
    {
              sh 'echo $ref'
    } else {
              sh 'echo notback'
    }
}


node('Frontend'){
   // if ( $ref == 'refs/heads/frontend')
   if (true)
    {
              sh 'echo $ref'
    } else {
              sh 'echo notfront'
    }
}
