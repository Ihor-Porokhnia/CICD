#!groovy

properties([disableConcurrentBuilds()])

node('Backend'){
    if (${ref} == 'refs/heads/backend') {
              sh 'echo back'
    } else {
              sh 'echo notback'
    }
}

node('Frontend'){
    if (${ref} == 'refs/heads/frontend') {
              sh 'echo front'
    } else {
              sh 'echo notfront'
    }
}
