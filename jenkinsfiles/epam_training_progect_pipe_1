#!groovy

properties([disableConcurrentBuilds()])

node('Backend'){
    if ('refs/heads/backend' == 'refs/heads/backend') {
              sh 'echo back $ref'
    } else {
              sh 'echo notback'
    }
}

node('Frontend'){
    if ('refs/heads/frontend' == 'refs/heads/frontend') {
              sh 'echo front $ref'
    } else {
              sh 'echo notfront'
    }
}
