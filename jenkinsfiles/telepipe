#!groovy
node {
 properties([
  pipelineTriggers([
   [$class: 'GenericTrigger',
    genericVariables: [
     [ key: 'actorname', value: '$.message.username' ],
     [ key: 'message', value: '$.message.text' ],
     
    ],
     
  
    token: 'tp',
    
    printContributedVariables: true,
    printPostContent: true,
    
    regexpFilterText: '$ref',
    regexpFilterExpression: '^refs/tags/.*'
   ]
  ])
 ])

 

}
