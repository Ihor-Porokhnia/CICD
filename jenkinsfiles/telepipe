#!groovy
node('master') {
 properties([
  pipelineTriggers([
   [$class: 'GenericTrigger',
    genericVariables: [
     [ key: 'actorname', value: '$.message.from.username' ],
     [ key: 'message', value: '$.message.text' ],
     
    ],
     
  
    token: 'tp',
    
    printContributedVariables: true,
    printPostContent: true,
    
   ]
  ])
 ])
//String pipe = "/pipe"
if (message.contains('pipe') {
   //message == message - pipe
  sh 'echo $message'
 }

}
